        layer_sym {
            bindings = <    
                &trans   &kp DE_GRAVE &kp DE_LT    &kp DE_GT    &kp DE_MINUS &kp DE_PIPE      TR_trans        &kp DE_CARET &kp DE_LBRC  &kp DE_RBRC  &kp DE_DLLR &kp DE_SECT  &trans
