/*              Col 6                   7                         8                         9       */
#define TR_1    &lt L_NUM ESC           &lt L_MEDIAMAUS ENTER     &lt L_MEDIAMAUS ENTER     &lt L_NUM ESC  
#define TR_2    &lt L_SYM BSPC          &none                     &none                     &lt L_SYM DELETE
#define TR_3    &lt L_FUNNAV DE_SPACE   &mo L_SETTINGS            &mo L_SETTINGS            &lt L_FUNNAV DE_SPACE

#define TR_2_beakl43    &lt L_SYM BSPC          &bslh_pipe             &none                     &lt L_SYM DELETE

/*   │ SYM │ BSPC│SPACE│ENTER│   │ENTER│SPACE│DEL  │ NAV │ */
